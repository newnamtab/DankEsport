@model EsportProject.Models.UserManagement

<h3>Add Role for @Model.Email</h3>
<div asp-validation-summary="All"></div>
<form asp-controller="UserManagement" asp-action="AddRole">
    <input asp-for="UserID" type="hidden"/>
    <select asp-for="NewRole" asp-items="Model.Rolelist"></select>
    <input type="submit" value="Add Role"/>
</form>