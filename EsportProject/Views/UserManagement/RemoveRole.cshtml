@model EsportProject.Models.UserManagement

<h3>Remove Role for @Model.Email</h3>
<div asp-validation-summary="All"></div>
<form asp-controller="UserManagement" asp-action="RemoveRole">
    <input asp-for="UserID" type="hidden" />
    <select asp-for="NewRole" asp-items="Model.Rolelist"></select>
    <input type="submit" value="Remove Role" />
</form>
