@{
    ViewData["Title"] = "Contact";
}
<center>
    <h1>How can we help?</h1>
    <h2>Ask us any question by using the contactform below</h2>
</center>
<br />
<div class="row">
    <div class="col-md-8">
        <form>
            <div id="contactform" class="form-group">
                <label for="email">Email address:</label>
                <input type="email" class="form-control" id="email">
            </div>
            <div class="form-group">
                <label for="pwd">Password:</label>
                <input type="password" class="form-control" id="pwd">
            </div>
            <button type="submit" class="btn btn-default">Submit</button>
        </form>
    </div>

    <div class="col-md-4">
        <a class="twitter-timeline" data-width="400" data-height="250" href="https://twitter.com/Kripparrian">Tweets by Kripparrian</a>
        <script async src="//platform.twitter.com/widgets.js" charset="utf-8"></script>
    </div>
</div>
   


