        <br />

        <div class="row">
            <div id="frontwidthimg" class="col-md-12">
               
                <div id="myCarousel" class="carousel slide" data-ride="carousel" data-interval="500" data-wrap="true">
                    <!-- Indicators -->
                    <ol class="carousel-indicators">
                        <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
                        <li data-target="#myCarousel" data-slide-to="1"></li>
                        <li data-target="#myCarousel" data-slide-to="2"></li>
                    </ol>

                    <!-- Wrapper for slides -->
                    <div class="carousel-inner">
                        
                        <div class="item active">
                            <img src="~/images/placeholderScoreboard0.png" id="frontsliderimg" class="img-responsive" alt="Picture1" />
                        </div>

                        <div class="item">
                            <img src="~/images/placeholderScoreboard1.png" id="frontsliderimg" class="img-responsive" alt="Picture2" />
                        </div>

                        <div class="item">
                            <img src="~/images/placeholderScoreboard2.png" id="frontsliderimg" class="img-responsive" alt="Picture3" />
                        </div>

                        <div class="item">
                            <img src="~/images/placeholderScoreboard3.png" id="frontsliderimg" class="img-responsive" alt="Picture4"/>
                        </div>

                        <div class="item">
                            <img src="~/images/placeholderScoreboard4.png" id="frontsliderimg" class="img-responsive" alt="Picture5" />
                        </div>
                    </div>

                    <!-- Left and right controls -->
                    <a class="left carousel-control" href="#myCarousel" data-slide="prev">
                        <span class="glyphicon glyphicon-chevron-left"></span>
                        <span class="sr-only">Previous</span>
                    </a>
                    <a class="right carousel-control" href="#myCarousel" data-slide="next">
                        <span class="glyphicon glyphicon-chevron-right"></span>
                        <span class="sr-only">Next</span>
                    </a>
                </div>
                
                
                

            </div>
        </div>

